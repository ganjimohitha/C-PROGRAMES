#include <stdio.h>

struct Student {
    char name[30];
    int roll;
    float marks;
};

int main() {
    struct Student s;
    printf("Enter name, roll, and marks: ");
    scanf("%s %d %f", s.name, &s.roll, &s.marks);
    printf("\nName: %s\nRoll: %d\nMarks: %.2f", s.name, s.roll, s.marks);
    return 0;
}
